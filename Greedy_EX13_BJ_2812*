// 백준 2812 왜 틀렸다고 뜨는지 모르겠다 :( Stack으로 풀어야하는 건가?

#include <iostream>
#include <algorithm>
#include <vector>
#include <math.h>

using namespace std;

vector<int> a;

int main(void) {
    
    int N, K, y, x, result = 0;
    cin >> N >> y;
    cin >> x;
    K = N - y;
    
    for(int n = N-1;n>=0;n--){
        int k;
        k = x/pow(10,n);
        a.push_back(k);
        x = x - k*pow(10,n);
    }
    sort(a.begin(), a.end(), greater<int>());
    
    for(int n=0;n<K;n++){
        result += a[n] * pow(10,K-n-1);
    }
    cout <<result << endl;
    return 0;
}
