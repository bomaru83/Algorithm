#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    
    int T;
    vector<int> result, result_size;
    cin >> T;
    
    for(int t = 0; t< T;t++){
        int x[41] = {0,}, N, M;
        vector<int> max_index;
        cin >> N >> M;
        for(int n = 1;n<=N;n++){
            for(int m = 1;m<=M;m++){
                x[n+m]++;
                
            }
        }
        int max = x[0];
        for(int i = 2;i<=N+M;i++){
            if(max < x[i]){
                max = x[i];
                max_index.clear();
                max_index.push_back(i);
            }
            else if(max == x[i])
                max_index.push_back(i);
        }
        result.insert(result.end(), max_index.begin(), max_index.end());
        int temp=0;
        for(int k=0;k<max_index.size();k++) temp++;
        result_size.push_back(temp);
        
    }
    int print=0;
    for(int r = 0; r < T;r++){
        cout << "#" << r+1 << " ";
        for(int k = print; k < print + result_size[r]; k++){
            cout << result[k] << " ";
        }
        print += result_size[r];
        cout << endl;
    }
    
    
    return 0;
}
