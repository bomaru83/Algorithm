//백준 1138
#include <iostream>
#include <vector>

using namespace std;
vector<int> ans;

int main(void){
    
    int n;
    cin >> n;
    for(int i = 0;i<n;i++)
        ans.push_back(0);
    
    for(int i = 0; i < n; i++){
        int x, count = 0;
        cin >> x;
        for(int j = 0;j<n;j++){
            
            if(count == x && ans[j] == 0){
                ans[j] = i+1;
                break;
            }
            if(ans[j] == 0)
                count++;
        }
    }
    for(int i = 0; i< n;i++){
        cout << ans[i] << " ";
    }
    cout << endl;
    return 0;
}
