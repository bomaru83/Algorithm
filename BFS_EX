#include <iostream>
#include <queue>
#include <vector>

using namespace std;


int num = 7;
int c[8];
vector<int> a[8];

void bfs(int start){
    queue<int> q;
    q.push(start);
    c[start] = true;
    
    while(!q.empty()){
        int x = q.front();
        q.pop();
        cout << x;
        for(int i = 0; i<a[x].size();i++){
            int y = a[x][i];
            if(!c[y]){  //방문을 한 상태가 아니라면
                q.push(y);
                c[y] = true;
            }
        }
        
    }
    
    
}

int main() {
    
    a[1].push_back(2);
    a[2].push_back(1);
    
    a[1].push_back(3);
    a[3].push_back(1);
    
    a[2].push_back(4);
    a[4].push_back(2);
    
    a[2].push_back(5);
    a[5].push_back(2);
    
    a[3].push_back(6);
    a[6].push_back(3);
    
    a[3].push_back(7);
    a[7].push_back(3);
    
    bfs(1);
    
    return 0;
}
