// 백준 11047
#include <iostream>
#include <vector>

using namespace std;
vector<int> a;

int main() {

    int n, k, result = 0;
    cin >> n >> k;
    for(int i = 0;i<n;i++){
        int x;
        cin >> x;
        a.push_back(x);
    }
    
    for(int i = a.size()-1;i>=0;i--){
        if(a[i]<k){
            result += k / a[i];
            k %= a[i];
        }
    }
    
    
    cout << result <<endl;
    return 0;
}
