

#include <iostream>
#include <vector>
#include <cmath>
#include <string>

using namespace std;

int main() {
    
    string x;
    cin >> x;
    for(int i =0;i<x.size();i++){
        if(x[i] == '_') continue;
        else if(x[i] >=97 && x[i] <= 122)   x[i]-=32;
    }
    cout << x;
    
    return 0;
}

