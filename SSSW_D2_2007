#include <iostream>
#include <vector>
#include <string>

using namespace std;

int main() {
    
    vector<int> result;
    int T;
    cin >> T;
    for(int t = 0; t<T;t++){
        string x;
        int check=0;
        cin >> x;
        int length = 1;
        for(int s=0;s<x.size();s++){
            if(x[0] == x[length]){
                check = 0;
                for(int k=0;k<length;k++){
                    if(x[k] == x[k+length])   check++;
                }
                if(check == length) break;
                else    length++;
            }
            else    length++;
            
        }
        
        
        
        result.push_back(length);
    }
    
    for(int i = 0;i<result.size();i++){
        cout << "#" << i+1 << " " << result[i] << endl;
    }
    
    
    return 0;
}
