#include <iostream>
#include <vector>

using namespace std;

int main() {
    vector<int> result;
    int T;
    cin >> T;
    for(int t=0;t<T;t++){
        int sum = 0;
        for(int i=0;i<10;i++){
            int x;
            cin >> x;
            if(x%2==1)
                sum += x;
            
        }
        result.push_back(sum);
        
    }
    
    for(int t=0;t<T;t++){
        cout << "#" << t+1 << " ";
        cout << result[t] << endl;
    }
    
    return 0;
}
